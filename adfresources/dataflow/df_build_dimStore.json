{
	"name": "df_build_dimStore",
	"properties": {
		"folder": {
			"name": "11-Exercise"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sql_stage_store",
						"type": "DatasetReference"
					},
					"name": "stageStore"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sql_dim_store",
						"type": "DatasetReference"
					},
					"name": "dimStore"
				}
			],
			"transformations": [
				{
					"name": "UpdateOrInsert"
				}
			],
			"scriptLines": [
				"source(output(",
				"          StoreName as string,",
				"          StoreType as string,",
				"          Description as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> stageStore",
				"stageStore alterRow(upsertIf(true())) ~> UpdateOrInsert",
				"UpdateOrInsert sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          StoreId as integer,",
				"          StoreName as string,",
				"          StoreType as string,",
				"          Description as string",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['StoreName'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> dimStore"
			]
		}
	}
}